<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="tk.jinhao.jhapplication"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <application
        android:allowBackup="true"
        android:name=".config.JHApplication"
        android:icon="@mipmap/by_renamatui27"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".module.main.MainActivity"
            android:label="@string/app_name">

        </activity>
        <activity android:name=".module.launch.GuideActivity"></activity>
        <activity android:name=".module.account.LoginActivity"></activity>

        <activity android:name=".module.launch.SplashActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>

    </application>

</manifest>
